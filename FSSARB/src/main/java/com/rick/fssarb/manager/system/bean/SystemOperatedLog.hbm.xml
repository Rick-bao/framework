<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.rick.fssarb.manager.system.bean">

	<class name="SystemOperatedLog" table="system_operated_log">

		<id name="id" type="java.lang.String" column="ID">
			<generator class="assigned" />
		</id>

		<property name="type" type="java.lang.String"
			column="TYPE" not-null="false" />

		<property name="description" type="java.lang.String"
			column="description" not-null="false" />

		<property name="createby" type="java.lang.String"
			column="createBy" not-null="false" />

		<property name="createtime" type="java.lang.String"
			column="createTime" not-null="false" />

		<property name="comments" type="java.lang.String"
			column="comments" not-null="false" />

	</class>
</hibernate-mapping>
