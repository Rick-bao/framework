<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.rick.fssarb.manager.system.bean">

	<class name="SystemUser" table="system_user">

		<id name="id" type="java.lang.String" column="ID">
			<generator class="assigned" />
		</id>

		<property name="name" type="java.lang.String"
			column="NAME" not-null="false" />

		<property name="nickname" type="java.lang.String"
			column="nickName" not-null="false" />

		<property name="password" type="java.lang.String"
			column="PASSWORD" not-null="false" />

		<property name="portraits" type="java.lang.String"
			column="portraits" not-null="false" />

		<property name="createby" type="java.lang.String"
			column="createBy" not-null="false" />

		<property name="createtime" type="java.lang.String"
			column="createTime" not-null="false" />

		<property name="enable" type="java.lang.String"
			column="ENABLE" not-null="false" />

		<property name="lastloginip" type="java.lang.String"
			column="lastLoginIP" not-null="false" />

		<property name="lastlogintime" type="java.lang.String"
			column="lastLoginTime" not-null="false" />

		<property name="comments" type="java.lang.String"
			column="comments" not-null="false" />

	</class>
</hibernate-mapping>
