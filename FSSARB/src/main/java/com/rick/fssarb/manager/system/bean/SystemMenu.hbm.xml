<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.rick.fssarb.manager.system.bean">

	<class name="SystemMenu" table="system_menu">

		<id name="id" type="java.lang.String" column="ID">
			<generator class="assigned" />
		</id>

		<property name="parent" type="java.lang.String"
			column="parent" not-null="false" />

		<property name="name" type="java.lang.String"
			column="NAME" not-null="false" />

		<property name="url" type="java.lang.String"
			column="URL" not-null="false" />

		<property name="menuicon" type="java.lang.String"
			column="menuIcon" not-null="false" />

		<property name="createby" type="java.lang.String"
			column="createBy" not-null="false" />

		<property name="createtime" type="java.lang.String"
			column="createTime" not-null="false" />

		<property name="enable" type="java.lang.String"
			column="ENABLE" not-null="false" />

		<property name="symbolicon" type="java.lang.String"
			column="symbolIcon" not-null="false" />

		<property name="updateby" type="java.lang.String"
			column="updateBy" not-null="false" />

		<property name="updatetime" type="java.lang.String"
			column="updateTime" not-null="false" />

		<property name="comments" type="java.lang.String"
			column="comments" not-null="false" />

	</class>
</hibernate-mapping>
