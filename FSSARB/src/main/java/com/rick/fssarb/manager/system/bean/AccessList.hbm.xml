<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.rick.fssarb.manager.system.bean">

	<class name="AccessList" table="access_list">

		<id name="id" type="java.lang.String" column="ID">
			<generator class="assigned" />
		</id>

		<property name="linkedroleid" type="java.lang.String"
			column="linkedRoleID" not-null="false" />

		<property name="rolename" type="java.lang.String"
			column="roleName" not-null="false" />

		<property name="linkedmenuid" type="java.lang.String"
			column="linkedMenuID" not-null="false" />

		<property name="menuname" type="java.lang.String"
			column="menuName" not-null="false" />

		<property name="menuurl" type="java.lang.String"
			column="menuURL" not-null="false" />

		<property name="createby" type="java.lang.String"
			column="createBy" not-null="false" />

		<property name="createtime" type="java.lang.String"
			column="createTime" not-null="false" />

		<property name="comments" type="java.lang.String"
			column="comments" not-null="false" />

	</class>
</hibernate-mapping>
