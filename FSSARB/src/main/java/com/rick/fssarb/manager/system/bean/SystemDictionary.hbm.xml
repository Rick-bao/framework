<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.rick.fssarb.manager.system.bean">

	<class name="SystemDictionary" table="system_dictionary">

		<id name="id" type="java.lang.String" column="ID">
			<generator class="assigned" />
		</id>

		<property name="parent" type="java.lang.String"
			column="parent" not-null="false" />

		<property name="name" type="java.lang.String"
			column="NAME" not-null="false" />

		<property name="code" type="java.lang.String"
			column="CODE" not-null="false" />

		<property name="serial" type="java.lang.Integer"
			column="SERIAL" not-null="false" />

		<property name="createby" type="java.lang.String"
			column="createBy" not-null="false" />

		<property name="createtime" type="java.lang.String"
			column="createTime" not-null="false" />

		<property name="comments" type="java.lang.String"
			column="comments" not-null="false" />

	</class>
</hibernate-mapping>
