<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.rick.fssarb.manager.system.bean">

	<class name="UserAccountAccess" table="user_account_access">

		<id name="id" type="java.lang.String" column="ID">
			<generator class="assigned" />
		</id>

		<property name="linkedtable" type="java.lang.String"
			column="linkedTable" not-null="false" />

		<property name="linkedid" type="java.lang.String"
			column="linkedID" not-null="false" />

		<property name="linkedrolelistid" type="java.lang.String"
			column="linkedRoleListID" not-null="false" />

		<property name="linkedgroupaccesslistid" type="java.lang.String"
			column="linkedGroupAccessListID" not-null="false" />

		<property name="linkedmenulistid" type="java.lang.String"
			column="linkedMenuListID" not-null="false" />

		<property name="createby" type="java.lang.String"
			column="createBy" not-null="false" />

		<property name="createtime" type="java.lang.String"
			column="createTime" not-null="false" />

		<property name="comments" type="java.lang.String"
			column="comments" not-null="false" />

	</class>
</hibernate-mapping>
