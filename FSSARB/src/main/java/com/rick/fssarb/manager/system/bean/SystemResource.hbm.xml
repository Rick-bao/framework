<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.rick.fssarb.manager.system.bean">

	<class name="SystemResource" table="system_resource">

		<id name="id" type="java.lang.String" column="ID">
			<generator class="assigned" />
		</id>

		<property name="linkedtable" type="java.lang.String"
			column="linkedTable" not-null="false" />

		<property name="linkedid" type="java.lang.String"
			column="linkedID" not-null="false" />

		<property name="name" type="java.lang.String"
			column="NAME" not-null="false" />

		<property name="realpath" type="java.lang.String"
			column="realPath" not-null="false" />

		<property name="savepath" type="java.lang.String"
			column="savePath" not-null="false" />

		<property name="url" type="java.lang.String"
			column="URL" not-null="false" />

		<property name="thumbnails" type="java.lang.String"
			column="thumbnails" not-null="false" />

		<property name="createby" type="java.lang.String"
			column="createBy" not-null="false" />

		<property name="createtime" type="java.lang.String"
			column="createTime" not-null="false" />

		<property name="enable" type="java.lang.String"
			column="ENABLE" not-null="false" />

		<property name="updatetime" type="java.lang.String"
			column="updateTime" not-null="false" />

		<property name="comments" type="java.lang.String"
			column="comments" not-null="false" />

	</class>
</hibernate-mapping>
